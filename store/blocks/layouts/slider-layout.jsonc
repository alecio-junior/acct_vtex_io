                "desktop": 5,
                "tablet": 1,
                "phone": 1
            },
            "infinite": true, //controle de deslize é infinito
            "showNavigationArrows": "desktopOnly", //apresenta as setas do slide apenas se o usuário estiver conectado à desktop
