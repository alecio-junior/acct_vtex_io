{
    /*
        slider principal da page home
    */
    "slider-layout#home": {
        "children": [
            "info-card#1",
            "info-card#2",
            "info-card#3",
            "info-card#4"
        ],
        "props": {
            "itemsPerPage": {
                "desktop": 1,
                "tablet": 1,
                "phone": 1
            },
            "autoplay": {
                "timeout": 5000,
                "stopOnHover": true
            },
            "showPaginationDots": "never" //não apresenta os pontos de navegação do slide
        }
    },
    "info-card#1": {
        "props": {
            "isFullModeStyle": true,
            "textPosition": "left",
            "imageUrl": "assets/quadrante_vtex_2021.png",
            "headline": "<br /><br /><br />Somos líder no Quadrante VTEX<br /><br />",
            "width": "100%",
            "callToActionText": "Saiba mais",
            "callToActionUrl": "/product",
            "blockClass": "info-card-example",
            "textAlignment": "center"
        }
    },
    "info-card#2": {
        "props": {
            "isFullModeStyle": true,
            "textPosition": "left",
            "imageUrl": "assets/slider-home-technology.jpg",
            "headline": "<br />TECHNOLOGY <br /><br /><br />Somos focados na solução de problemas altamente complexos.<br /><br /><br />",
            "width": "100%",
            "callToActionMode": "none",
            "blockClass": "info-card-example",
            "textAlignment": "center"
        }
    },
    "info-card#3": {
        "props": {
            "isFullModeStyle": true,
            "textPosition": "left",
            "imageUrl": "assets/trabalhadora6.png",
