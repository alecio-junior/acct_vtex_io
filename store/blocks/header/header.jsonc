{
  "header": {
    "blocks": [
      "header-layout.desktop",
      "header-layout.mobile"
    ]
  },
  "header.full": {
    "blocks": [
      "header-layout.desktop",
      "header-layout.mobile"
    ]
  },
  "header-layout.desktop": {
    "children": [
      "sticky-layout#desktop"
    ]
  },
  "sticky-layout#desktop": {
    "props": {
      "blockClass": "sticky-header"
    },
    "children": [
      "flex-layout.row#desktop"
    ]
  },
  "flex-layout.row#desktop": {
    "props": {
      "horizontalAlign": "center",
      "verticalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": true,
      "marginTop": 3,
      "blockClass":"header"
    },
    "children": [
      "flex-layout.col#logo-desktop",
      "flex-layout.col#spacer1",
      "flex-layout.row#spacer1",
      "flex-layout.col#spacer2",
      "flex-layout.row#spacer2"
    ]
  },
  "flex-layout.col#spacer1": {
    "props": {
      "width": "grow"
    }
  },
  "flex-layout.row#spacer1": {
    "props": {
      "marginTop": 6
    },
    "children": [
      "vtex.menu@2.x:menu#websites"
    ]
  },
  "flex-layout.col#spacer2": {
    "props": {
      "width": "grow"
    }
  },
  "flex-layout.row#spacer2": {
    "props": {
      "marginTop": 6
    },
    "children": [
      "vtex.menu@2.x:menu#locale"
    ]
  },
  "flex-layout.col#logo-desktop": {
    "props": {
      "verticalAlign": "middle"
    },
    "children": [
      "logo"
    ]
  },
  "logo": {
    "props": {
      "title": "Logo",
      "href": "/",
      "url": "assets/logo-acct-black.png",
      "width": 150,
      "height": 40
    }
  },
  "vtex.menu@2.x:menu#websites": {
    "props": {
      "experimentalOptimizeRendering": true
    },
    "children": [
      "menu-item#home",
      "menu-item#acct",
      "menu-item#oquefazemos",
      "menu-item#cadastre-se",
      "login",
      "minicart.v2"
    ]
  },
  "menu-item#home": {
    "props": {
      "id": "menu-item-home",
      "type": "custom",
      "highlight": true,
      "itemProps": {
        "type": "internal",
        "href": "/",
        "noFollow": false,
        "tagTitle": "home",
        "text": "Home"
      }
    }
  },
  "menu-item#acct": {
    "props": {
      "id": "menu-item-acct",
      "type": "custom",
      "highlight": true,
      "itemProps": {
        "type": "internal",
        "href": "/",
        "noFollow": false,
        "tagTitle": "acct",
        "text": "ACCT"
      }
    },
    "blocks": [
      "vtex.menu@2.x:submenu#categories"
    ]
  },
  "vtex.menu@2.x:submenu#categories": {
    "children": [
      "vtex.menu@2.x:menu#sobre"
    ]
  },
